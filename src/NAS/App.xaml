    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <!--  Attach default Theme  -->
        <ResourceDictionary Source="pack://application:,,,/Fluent;Component/Themes/Generic.xaml" />
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
